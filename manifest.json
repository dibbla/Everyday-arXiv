{
    "manifest_version": 3,
    "name": "Read it later",
    "description": "Save and organize papers/paper URLs for reading later",
    "version": "0.1",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/Hi_icon.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage"
    ]
    
  }